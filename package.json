    "css-loader": "latest",
    "eslint": "latest",
    "file-loader": "3.0.1",
    "html-webpack-plugin": "latest",
    "jest": "latest",
    "prettier": "latest",
    "sass-loader": "latest",
    "style-loader": "latest",
    "sucrase": "latest",
    "ts-jest": "latest",
    "ts-loader": "latest",
    "tsconfig-paths-webpack-plugin": "latest",
    "tslint": "latest",
    "typescript": "latest",
    "typescript-eslint-parser": "^21.0.2",
    "typescript-tslint-plugin": "^0.1.2",
    "url-loader": "1.1.2",
    "webpack": "latest",
